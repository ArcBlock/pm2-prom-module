{
    "name": "pm2-prom-module",
    "version": "1.0.3",
    "description": "PM2 module to help collect applications statistic and send it to Prometheus server",
    "main": "index.js",
    "dependencies": {
        "pm2": "^5.2.2",
        "pmx": "beta",
        "prom-client": "^15.1.0"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "watch": "tsc -w -p tsconfig.json"
    },
    "files": [
        "README.md",
        "**/*.js"
    ],
    "apps": [
        {
            "merge_logs": true,
            "max_memory_restart": "100M",
            "script": "index.js"
        }
    ],
    "config": {
        "port": "9988",
        "service_name": "",
        "debug": false
    },
    "devDependencies": {
        "@types/node": "^18.19.3",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">=14"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/VeXell/pm2-prom-module.git"
    },
    "author": "Viacheslav Volkov (vexell@gmail.com)",
    "license": "MIT",
    "keywords": [
        "PM2",
        "Prometheus",
        "Metrics",
        "Monitoring",
        "Prom client",
        "Module",
        "Node.js",
        "Javascript"
    ]
}
